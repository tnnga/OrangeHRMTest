<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="OrangeHRMSuite" verbose="1">
    <parameter name="browserType" value="chrome" />
    <parameter name="webURL"
               value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login" />
    <test name="OrangeHRMSignInTest">
        <classes>
            <class name="com.nnga.testcases.SignInTest">
                <methods>
                    <include name="signPage"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>